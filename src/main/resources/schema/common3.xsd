	<xs:complexType name="SpeedProfileEtcs">
		<xs:sequence/>
		<xs:attribute name="etcsTrainCategoryNumber" use="required" type="rail3:tEtcsTrainCategoryNumber">
			<xs:annotation>
				<xs:documentation>this information shall be derived from trainType, airBrakeApplicationPosition and maxCantDeficiency. Shall not be used as leading information, but rather as derived additional description.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="SpeedProfileLoad">
		<xs:sequence/>
		<xs:attribute name="exceedsAxleLoad" use="optional" type="rail3:tWeightTons">
