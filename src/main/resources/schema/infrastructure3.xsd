				</xs:attribute>
				<xs:attribute name="belongsToBaliseGroup" use="optional" type="rail3:tRef">
					<xs:annotation>
						<xs:documentation>reference to the balise group that this balise belongs to</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="distanceToPredecessorBaliseWithinGroup" use="optional" type="rail3:tLengthM">
					<xs:annotation>
