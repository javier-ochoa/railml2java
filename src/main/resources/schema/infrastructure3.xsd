		<xs:attribute name="duplicate" use="optional" type="rail3:tBaliseDuplicate">
			<xs:annotation>
				<xs:documentation>corresponds to ETCS variable M_DUP</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mVersion" use="optional" type="xs:nonNegativeInteger">
			<xs:annotation>
				<xs:documentation>corresponds to ETCS variable M_VERSION</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BaliseGroup">
