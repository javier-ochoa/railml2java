		<xs:attribute name="locationAccuracy" use="optional" type="rail3:tLengthM">
			<xs:annotation>
				<xs:documentation>corresponds to ETCS variable Q_LOCACC</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="mVersion" use="optional" type="xs:nonNegativeInteger">
			<xs:annotation>
				<xs:documentation>corresponds to ETCS variable M_VERSION</xs:documentation>
